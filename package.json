  "scripts": {
    "build": "cross-env NODE_ENV=production next build",
    "build-storybook": "build-storybook",
    "check": "npm-run-all --parallel lint typecheck test",
    "clean": "rimraf node_modules .next && npm install --legacy-peer-deps",
    "create-index": "ts-node --esm src/lib/mongodb/create-unique-index.ts",
    "dev": "next dev",
    "format": "prettier --write . --log-level warn",
    "generate": "ts-node src/lib/generate-sample-csv.ts",
    "lint": "eslint --config eslint.config.mjs .",
    "lint:fix": "eslint --fix --config eslint.config.mjs .",
    "prepare": "husky install",
    "preview": "next start",
    "reinstall": "npm install --legacy-peer-deps",
    "reset": "rimraf package-lock.json node_modules .next && npm cache clean --force && npm install --legacy-peer-deps",
    "start": " cross-env NODE_ENV=production next start",
    "storybook": "start-storybook -p 6006",
    "test": "jest -u",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "test:json": "jest --json --outputFile=jest-results.json || true",
    "test:report": "npm run test:json && node ./.wescore/scripts/parse-jest-results.mjs",
